<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kampusku">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Kampusku"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kampusku"> <!-- Ganti ke Theme.Kampusku -->

        <activity android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".DashboardActivity"/>
        <activity android:name=".ListMahasiswaActivity"/>
        <activity android:name=".InputMahasiswaActivity"/>
        <activity android:name=".EditMahasiswaActivity"/>
        <activity android:name=".DetailMahasiswaActivity"/>
        <activity android:name=".InformasiActivity"/>


    </application>
</manifest>
